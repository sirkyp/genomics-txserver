{
  "name": "node-shl-api",
  "version": "0.3.0-SNAPSHOT",
  "description": "Node.js Smart Health Link API with SQLite database",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "test:shl": "jest --testPathPattern=shl",
    "test:vcl": "jest --testPathPattern=vcl",
    "test:xig": "jest --testPathPattern=xig",
    "test:packages": "jest --testPathPattern=packages",
    "test:token": "jest --testPathPattern=token",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-pattern 'static/assets/js/html5shiv.js'",
    "lint:relaxed": "eslint . --config .eslintrc.relaxed.js",
    "install-global": "npm link",
    "list": "node tx-import.js list",
    "import": "node tx-import.js",
    "tx-import": "node tx/importers/tx-import.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "base45": "^3.0.0",
    "bcrypt": "^6.0.0",
    "cbor": "^9.0.1",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "commander": "^9.4.1",
    "commonmark": "^0.31.2",
    "connect-sqlite3": "^0.9.16",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.2",
    "fast-xml-parser": "^5.3.4",
    "fhir-validator-wrapper": "1.2.1",
    "fhirpath": "^4.8.0",
    "fs-extra": "^11.1.0",
    "inquirer": "^8.2.5",
    "liquidjs": "^10.24.0",
    "lusca": "^1.7.0",
    "natural": "^6.12.0",
    "node-cron": "^3.0.3",
    "pako": "^2.1.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "properties-file": "^3.6.0",
    "rimraf": "^5.0.10",
    "sqlite3": "^5.1.6",
    "tar": "^7.5.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.57.1",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mock-fs": "^5.2.0",
    "nock": "^13.4.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "tmp": "^0.2.4"
  },
  "jest-junit": {
    "outputDirectory": "./test-results",
    "outputName": "junit.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º "
  },
  "keywords": [
    "fhir",
    "shc",
    "shl",
    "xig",
    "token",
    "oauth",
    "api-keys",
    "smart-health-link",
    "smart-health-card"
  ],
  "author": "",
  "license": "MIT",
  "overrides": {
    "tar": "^7.5.7",
    "fast-xml-parser": "^5.3.4"
  },
  "bin": {
    "tx-import": "./tx-import.js"
  }
}